pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
grid = {}

function _init()
  for y = 0, 127 do
    for x = 0, 127 do
      grid[x+y*128] = rnd() > 0.5
    end
  end
end

function _draw()
  cls()
  for y = 0, 127 do
    for x = 0, 127 do
      if grid[x+y*128] then
        pset(x, y, 1)
      end
    end
  end
end
